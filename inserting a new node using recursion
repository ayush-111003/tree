#include<bits/stdc++.h>
using namespace std;
struct node
{
    node *lchild , *rchild;
    int data;
};

node *insert(node *p,int key)
{
    node *t;
    if(p==NULL)
    {
        t=new node;
        t->data = key;
        t->lchild = t->rchild=NULL;
        // t=root;
        return t; 
    }
    if(key<p->data)
        p->lchild=insert(p->lchild,key);
    else if(key>p->data)
        p->rchild=insert(p->rchild,key);
    
    return p;
}
void inorder(node *p)
{
    if(p)
    {
        inorder(p->lchild);
        cout<<p->data<<" ";
        inorder(p->rchild);
    }
}
